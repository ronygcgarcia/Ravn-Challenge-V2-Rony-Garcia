model Category {
    id      Int       @id @default(autoincrement())
    name    String    @unique
    product Product[]

    @@map("categories")
}

model Product {
    id          Int      @id @default(autoincrement())
    name        String   @unique
    price       Float
    category    Category @relation(fields: [category_id], references: [id])
    category_id Int

    @@map("products")
}
